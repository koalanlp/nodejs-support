{
  "name": "koalanlp",
  "version": "2.1.3",
  "description": "Node.js wrapper for KoalaNLP (An integrated API for processing Korean in with Kotlin/Java/Scala)",
  "author": {
    "name": "koalanlp"
  },
  "repository": {
    "type": "git",
